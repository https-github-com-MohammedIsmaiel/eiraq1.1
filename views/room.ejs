        <div class="cont">

            <!----------------- Start header ------------------->
            <header class="container-fluid boxedit head">
                <div class="username">
                    <% if (typeof pro_img=='undefined' ) { %>
                        <img src="/upload_images/default.png" width="50px"
                            alt="user image" srcset="">

                        <% } else if (pro_img.startsWith("https")) { %>

                            <img src="<%=pro_img%>" width="50px"
                                alt="user image" srcset="">
                            <% } else { %>
                                <img src="/upload_images/<%=pro_img%>"
                                    width="50px" alt="user image" srcset="">

                                <% } %>
                                    <% if (isHost) { %>
                                        <span id="logedInUser">
                                            <%= loggedinuser+" (host)"%>
                                        </span>
                                        <% } else { %>
                                            <span id="logedInUser">
                                                <%= loggedinuser%>
                                            </span>
                                            <% } %>
                </div>
                <div class="logo">
                    <div class="contain">
                        <div class="box">
                            <div class="title">
                                <span class="block"></span>
                                <h1>Eiraq<span></span></h1>
                            </div>
                            <div class="role">
                                <div class="block"></div>
                                <p>Conference App</p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="display">
                    <button onclick="changeseem()" class="btn"><i
                            class="fas fa-th-large"></i></button>
                    <button onclick="blackmoon()" class="btn"><i id="black"
                            class="fas fa-adjust"></i></button>
                </div>
            </header>
            <!----------------- End header ------------------->

            <!----------------- Start Aside ------------------->
            <div class="mainrightside">
                <!-- fatma khaled -->
                <div class="meeting-info meeting-d-none">
                    <% if(typeof meetinginfo!='undefined' ) { %>
                        <label>Meeting Id: <input type="text"
                                value=" <%= meetinginfo.meetingid%>"
                                style="background-color: #EEE; border:none;"></label>
                        <br>
                        <label>Host Name: <input type="text"
                                value="<%= meetinginfo.hostname%>"
                                style="background-color: #EEE; border:none;">
                        </label>
                        <br>
                        <label>Passcode: <input type="text"
                                value="<%= meetinginfo.password%>"
                                style="background-color: #EEE; border:none;"></label>
                        <br>
                        <label>Url: <input id="Url"
                                value="<%= meetinginfo.meetingurl%>" type="text"
                                class=" border-0 ">
                            <button
                                class=" Copy far fa-copy border-0 border-color:#EEE "
                                onclick="copy();"></button></span></label><span>
                            <% } %>
                </div>
                <!-- fatma khaled -->
                <button id="meet-inf" onclick="dispMeetingInfo()"><i
                        class="x far fa-lightbulb"></i></button>
                <!-- mohammed ismael -->
                <button id="btnroom" class="btn" onclick="rooms()">
                    <i class="fad fa-angle-double-right"></i>
                </button>
                <!--mohammed gamal -->
                <button class="btn" id="attendence">
                    <i class="far fa-file-alt"></i>
                </button>
                <!-- mohammed gamal -->
