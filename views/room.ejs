                </button>
                <!-- mohammed ismael -->
                <button id="users" onclick="" class="btn">
                    <i class="fas fa-users"></i>
                </button>
                <!-- ismael mute all -->
                <% if (isHost) { %>
                    <button id="muteAll" class="btn">
                        <i class="fas fa-microphone-alt"></i>
                    </button>
                    <% }%>
            </div>
            <!----------------- End Aside ------------------->

